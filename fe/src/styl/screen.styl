[aria-label="breadcrumb"]
  margin 1em 0

.link-list
  display flex
  flex-wrap wrap

  .btn
    flex 0 1 19.2%
    font-size 12px
    margin-left 1%
    word-break break-all

    &:nth-child(5n+1)
      margin-left 0

    &:nth-child(n+6)
      margin-top .5rem

.preview
  margin-top 1rem
  img
    max-width 100%
